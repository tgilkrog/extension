{
    "manifest_version": 3,
    "name": "Wextension",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html"
    },
    "permissions": ["storage", "tabs"],
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'; style-src 'self' https://cdnjs.cloudflare.com;"
    }
  }
  